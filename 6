#define _CRT_SECURE_NO_WARNINGS 1
/*编写一个程序，只接受正整数输入，然后显示所有小于
或等于该数的素数*/

#include<stdio.h>

int main()
{
	int num = 0;
	scanf("%d", &num);
	int i = 1;
	while (i <= num)
	{
		int m = 0;
		for(int j=2;j<i;j++)
		{
			if (i % j == 0)
				m++;
		}
		if(0==m)
			printf("%d ", i);
		i++;
	}
	return 0;

}
